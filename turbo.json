{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "lint": {
      "cache": false,
      "dependsOn": ["^lint"]
    },
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": ["^test"]
    }
  }
}
